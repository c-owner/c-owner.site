{
    "arrowParens": "always",
    "endOfLine": "lf",
	"printWidth": 100,
	"htmlWhitespaceSensitivity": "css",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": false,
	"vueIndentScriptAndStyle": true,
	"bracketSameLine": true,
	"bracketSpacing": true,
	"overrides": [
		{
			"files": ["*.js", "*.ts", "*.tsx", "*.jsx"],
			"options": {
				"semi": false
			}
		},
		{
			"files": "*.json",
			"options": {
				"semi": false,
				"bracketSpacing": true,
				"trailingComma": "all"
			}
		}
	]
}
